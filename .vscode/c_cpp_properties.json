{
    "configurations": [
        {
            "name": "Windows",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/include",
                "${workspaceFolder}/src",
                "${command:cmake.getLaunchTargetDirectory}"
            ],
            "defines": [
                "_WIN32",
                "_WIN64",
                "_M_X64",
                "NOMINMAX",
                "_CRT_SECURE_NO_WARNINGS"
            ],
            "intelliSenseMode": "windows-msvc-x64",
            "windowsSdkVersion": "10.0.26100.0",
            "cStandard": "c17",
            "cppStandard": "c++20",
            "compilerArgs": [
                "/std:c++20",
                "/await",
                "/permissive-"
            ],
            "browse": {
                "limitSymbolsToIncludedHeaders": true
            },
            "configurationProvider": "ms-vscode.cmake-tools"
        },
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/include",
                "${workspaceFolder}/src",
                "${command:cmake.getLaunchTargetDirectory}"
            ],
            "defines": [],
            "intelliSenseMode": "linux-gcc-x64",
            "cStandard": "c17",
            "cppStandard": "c++20",
            "configurationProvider": "ms-vscode.cmake-tools"
        },
        {
            "name": "macOS",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/include",
                "${workspaceFolder}/src",
                "${command:cmake.getLaunchTargetDirectory}"
            ],
            "defines": [],
            "intelliSenseMode": "macos-clang-x64",
            "cStandard": "c17",
            "cppStandard": "c++20",
            "configurationProvider": "ms-vscode.cmake-tools"
        }
    ],
    "version": 4
}